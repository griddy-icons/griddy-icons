{
  "name": "my-icons",
  "version": "0.1.0",
  "description": "A minimal React icon library inspired by phosphor-react, with multiple weights and a simple build.",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "phosphor-react",
    "library"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "build": "tsup",
    "clean": "rm -rf dist src/icons src/index.ts && mkdir -p src/icons",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  }
}
