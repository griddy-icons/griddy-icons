{
  "name": "griddy-icons",
  "version": "0.2.0",
  "description": "Griddy Icons is a free, open-source icon library with a unique utilitarian vibe ready for your next project.",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Zuzana Benova",
    "email": "zuzanabenova.be@gmail.com",
    "url": "https://www.thegistof.me/zuzka"
  },
  "repository": "https://github.com/griddy-icons/griddy-icons",
  "homepage": "https://www.griddyicons.com",
  "bugs": {
    "url": "https://github.com/griddy-icons/griddy-icons/issues"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "griddy",
    "design",
    "interface",
    "UI",
    "UX"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18",
    "react": "^18",
    "tsup": "^8",
    "tsx": "^4",
    "typescript": "^5"
  },
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "build": "npm run generate && tsup && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=16"
  }
}
